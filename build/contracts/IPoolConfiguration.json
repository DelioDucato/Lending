urns\":{\"_0\":\"the collateral percent\"}},\"getLiquidationBonusPercent()\":{\"returns\":{\"_0\":\"the liquidation bonus percent\"}},\"getOptimalUtilizationRate()\":{\"returns\":{\"_0\":\"the optimal utilization\"}},\"getUtilizationRate(uint256,uint256)\":{\"params\":{\"_totalBorrows\":\"the total borrows of the ERC20 token pool\",\"_totalLiquidity\":\"the total liquidity of the ERC20 token of the pool\"},\"returns\":{\"utilizationRate\":\"the utilization rate of the ERC20 pool\"}}},\"title\":\"Pool configuration interface\",\"version\":1},\"userdoc\":{\"kind\":\"user\",\"methods\":{\"calculateInterestRate(uint256,uint256)\":{\"notice\":\"calculate the annual interest rate\"},\"getBaseBorrowRate()\":{\"notice\":\"get base borrow rate of the ERC20 token pool\"},\"getCollateralPercent()\":{\"notice\":\"get the collateral percent which is the percent that the liquidity can use as collateral\"},\"getLiquidationBonusPercent()\":{\"notice\":\"get the liquidation bonus percent to calculate the collateral amount of liquidation\"},\"getOptimalUtilizationRate()\":{\"notice\":\"get optimal utilization rate of the ERC20 token pool\"},\"getUtilizationRate(uint256,uint256)\":{\"notice\":\"calculate the utilization rate\"}},\"notice\":\"The interface of pool configuration of the ERC20 token pool\",\"version\":1}},\"settings\":{\"compilationTarget\":{\"/D/Lending-Development/Lending/contracts/interfaces/IPoolConfiguration.sol\":\"IPoolConfiguration\"},\"evmVersion\":\"istanbul\",\"libraries\":{},\"metadata\":{\"bytecodeHash\":\"ipfs\"},\"optimizer\":{\"enabled\":true,\"runs\":200},\"remappings\":[]},\"sources\":{\"/D/Lending-Development/Lending/contracts/interfaces/IPoolConfiguration.sol\":{\"keccak256\":\"0x319eb9525ca64ec0c32db02424d33800cc4d5762d11cef025996501f6b8e6ecd\",\"urls\":[\"bzz-raw://bd10793044e98dd5cb4183f87f57a27cd7fa0d6f9998592b76564459c5196adb\",\"dweb:/ipfs/QmZMHL7GCwGnpx4TtV3cvNdzEHv4jweXrekpovQkEutmtj\"]}},\"version\":1}",
